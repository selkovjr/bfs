[
  {
    "settings": [ "master" ],
    "app index": {
      "verbs": ["get"],
      "path": "/",
      "call": "pageframe.index"
    },
    "app login": {
      "verbs": ["get"],
      "path": "/login",
      "call": "@Login.index"
    },
    "login_submit": {
      "verbs": ["post"],
      "path": "/login_submit",
      "call": "@Login.submit"
    },
    "bird lookup": {
      "verbs": ["get"],
      "path": "/bird",
      "call": "@Samples.bird"
    },
    "location lookup": {
      "verbs": ["get"],
      "path": "/location",
      "call": "@Locations.find"
    }
  }
]
